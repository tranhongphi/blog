<!DOCTYPE html>
<html lang="en">

<head>
    <title>DashBoard</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>

<body>

    <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
        <!-- Brand/logo -->
        <a class="navbar-brand" href="/admin">Blog Admin</a>

        <!-- Links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link active" href="/admin/post">Post</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/admin/user">User</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="#">Link 3</a>
            </li>
        </ul>
    </nav>

    <div class="panel panel-default">
        <div class="panel-heading p-3 bg-primary text-center text-white">
            Users
        </div>
        <!-- /.panel-heading -->
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr class="bg-secondary">
                            <th>ID</th>
                            <th>Email</th>
                            <th>First name</th>
                            <th>Last name</th>
                            <th>Create time</th>
                            <th>Update time</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% if(data && data.users) {%>
                            <% for(var i=0; i < data.users.length; i++) {%>
                                <tr>
                                    <td>
                                        <%= data.users[i].id %>
                                    </td>
                                    <td>
                                        <%= data.users[i].email %>
                                    </td>
                                    <td>
                                        <%= data.users[i].first_name %>
                                    </td>
                                    <td>
                                        <%= data.users[i].last_name %>
                                    </td>
                                    <td>
                                        <%= data.users[i].created_at %>
                                    </td>
                                    <td>
                                        <%= data.users[i].updated_at %>
                                    </td>
                                </tr>
                                <% } %>
                                    <% } %>
                    </tbody>
                </table>
            </div>
            <!-- /.table-responsive -->
        </div>
        <!-- /.panel-body -->
    </div>
    <!-- /.panel -->


    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
</body>

</html>